
{{#stories/story-base title=title subTitle=subTitle color="-white" width="-w2" height="-h2" data-ss-colspan="2"}}
    <main class="stat-notices-recent">
        {{#stories/story-slider loaded=loaded}}
            {{#stories/story-slide}}
                <div class="stat-notices-recent__welcome">
                    <h3 class="-delta">StatNoticesRecent</h3>
                    {{#link-to 'statnotices'}}<div>Open Stat Notices Route</div>{{/link-to}}
                    <div class="js-open-statnotices" {{action "switchToStatNotices"}}>Open Stat Notices</div>
                </div>                   
            {{/stories/story-slide}}            
            {{#each item in items}}
                {{#stories/story-slide}}
                    <div class="stat-notices-recent__item">
                        <p>This page's context item was loaded from Leeds Data Mill</p>
                        <p>Some sample object properties...</p>
                        <ul>
                            <li>street: {{item.street}}</li>
                            <li>lat: {{item.lat}}</li>
                            <li>lng: {{item.lng}}</li>
                            <li>colour: {{item.colour}}</li>
                        </ul>
                    </div>
                {{/stories/story-slide}}
            {{/each}}
        {{/stories/story-slider}}
    </main>
{{/stories/story-base}}
