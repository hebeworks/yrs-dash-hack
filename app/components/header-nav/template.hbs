<nav class="nav">
	<ul class="js-site-nav nav__list -main -header">
	    {{#each item in navItems}}
	    <li {{bind-attr class="item.dashed"}}>
	        {{nav-icon title=item.title jshook=item.jshook iconclass=item.iconclass svgclass=item.svgclass action=item.action route=item.route}}
	    </li>
	    {{/each}}
	</ul>
</nav>

{{#if showSubNav}}
<div class="site-sub-nav">
    {{header-nav navItems=navSubItems}}
</div>
{{/if}}


{{yield}}
